{
    "name": "exolnet/glutenblocks",
    "description": "Collection of Wordpress blocks for the Gutenberg editor.",
    "keywords": [
        "exolnet",
        "laravel",
        "glutenblocks"
    ],
    "homepage": "https://github.com/eXolnet/glutenblocks",
    "license": "MIT",
    "authors": [
        {
            "name": "eXolnet",
            "homepage": "https://www.exolnet.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3.2"
    },
    "scripts": {
        "lint": "vendor/bin/phpcs -p --standard=PSR2 config src tests",
        "lint-fix": "vendor/bin/phpcbf -p --standard=PSR2 config src tests",
    },
    "config": {
        "sort-packages": true
    }
}
